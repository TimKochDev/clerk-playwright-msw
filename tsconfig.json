{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015", "DOM", "ES2021.Intl"],
    "module": "commonjs",
    "outDir": "./build/",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true
  }
}
